import MyComponent from '.';

const createComponent = TestUtils.makeCreateComponent(MyComponent);

describe('MyComponent', () => {
  let component, testRenderer;

  beforeEach(() => {
    [component, testRenderer] = createComponent();
  });
  afterEach(() => {
    component = null;
    testRenderer = null;
  });

  it('renders', () => {
