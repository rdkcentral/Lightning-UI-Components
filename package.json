{
  "name": "@lightning/lightning-ui",
  "version": "0.0.0",
  "dependencies": {
    "wpe-lightning": "git+https://github.com/rdkcentral/Lightning.git#1.7.0",
    "wpe-lightning-sdk": "git+https://github.com/rdkcentral/Lightning-SDK.git#2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/config-lerna-scopes": "^8.3.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/html": "^5.3.19",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "canvas": "^2.6.1",
    "chalk": "^2.4.2",
    "child_process": "^1.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "codesandbox": "^2.1.16",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-system": "^2.2.2",
    "fs-extra": "^8.1.0",
    "gh-pages": "^3.0.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^6.5.2",
    "jest": "^24.9.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "jest-webgl-canvas-mock": "^0.2.3",
    "lerna": "^3.22.1",
    "lint-staged": "^9.5.0",
    "path": "^0.12.7",
    "prettier": "^1.19.1",
    "react-styleguidist": "^9.2.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-config-utils": "^2.3.1",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "jest --findRelatedTests"
    ]
  },
  "repository": {
    "url": "https://github.comcast.com/Lightning/lightning-ui"
  },
  "scripts": {
    "postinstall": "npm run bootstrap",
    "prebuild": "npm run bootstrap",
    "build": "build-storybook -c .storybook -o dist",
    "bootstrap": "lerna bootstrap",
    "lint": "npm run lint:js",
    "lint:js": "eslint './components/**/*.js'",
    "lerna": "lerna",
    "build:dist": "lerna exec rm -- -rf dist && lerna run build --stream",
    "gh-pages": "gh-pages -d dist",
    "release": "env-cmd lerna version --no-commit-hooks",
    "prepare": "npm run build:dist",
    "publish": "lerna publish from-package --no-commit-hooks",
    "new": "node utils/scripts/add-component.js",
    "new:story": "sh utils/scripts/make-story.sh",
    "start": "start-storybook",
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "registry": "https://artifactory.comcast.com/artifactory/api/npm/Lightning-npm-releases/"
  }
}
