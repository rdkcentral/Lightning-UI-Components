{
  "name": "@lightning/ui",
  "version": "5.0.0-next.60",
  "sideEffects": false,
  "dependencies": {
    "csv-writer": "^1.6.0",
    "debounce": "^1.2.1",
    "fast-csv": "^4.3.6"
  },
  "peerDependencies": {
    "@lightningjs/core": "^2.1.1"
  },
  "browser": "index.js",
  "main": "index.js",
  "types": "index.d.ts",
  "bin": {
    "bootstrap": "bin/bootstrap.sh"
  },
  "files": [
    "index.js",
    "Styles.js",
    "Base.js",
    "Base.d.ts",
    "GlobalUpdateManager.js",
    "{bin,elements,extensions,layout,mixins,patterns,suite,Styles,textures,utils,themes,context}/**/*",
    "!{elements,extensions,layout,mixins,patterns,suite,Styles}/**/*.stories.js",
    "!{elements,extensions,layout,mixins,patterns,suite,textures,utils}/**/*.test.js",
    "test/lightning-test-renderer.js",
    "test/lightning-test-utils.js",
    "!public/"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@lightning/lng-typed": "^1.2.3",
    "@lightningjs/core": "2.5.0",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-docs": "^6.5.3",
    "@storybook/addon-essentials": "^6.5.3",
    "@storybook/addon-storysource": "^6.5.3",
    "@storybook/html": "^6.5.3",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "esm": "^3.2.25",
    "fast-glob": "^3.2.7",
    "gh-pages": "^4.0.0",
    "glob": "^7.2.0",
    "husky": "^8.0.1",
    "jest": "^26.6.1",
    "jest-webgl-canvas-mock": "^0.2.3",
    "line-reader-sync": "^0.1.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-release": "^19.0.2",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "./{collections,elements,layout,mixins,patterns,suite,Styles,textures,utils,themes,context}/**/*.js": [
      "npm run lint"
    ]
  },
  "repository": {
    "url": "https://github.comcast.com/Lightning/lightning-ui"
  },
  "scripts": {
    "generateStorybookData": "node ./scripts/storybook/generate && npm run styleDocs",
    "build": "npm run generateStorybookData && build-storybook -c .storybook -o dist && node -r esm bin/translationScript.js",
    "styleDocs": "node -r esm bin/translationScript.js",
    "lint": "eslint './{collections,elements,layout,mixins,patterns,suite,Styles,styles,textures,utils,themes,context}/**/*.js'",
    "lint:fix": "eslint --fix './{collections,elements,layout,mixins,patterns,suite,Styles,styles,textures,utils,themes,context}/**/*.js'",
    "gh-pages": "gh-pages -d dist",
    "create": "node bin/create.js",
    "start": "npm run generateStorybookData && start-storybook --no-manager-cache -p 8000",
    "start:ci": "start-storybook --ci --smoke-test",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:watch": "jest --no-coverage --watch",
    "kick-concourse": "git commit --no-verify --allow-empty -m 'Trigger Concourse' && git push origin",
    "prepare": "husky install",
    "release": "git fetch origin && git checkout develop && git pull origin develop && git pull origin release && git push origin develop && git checkout release && git pull origin release && git pull origin develop && git push origin release",
    "update:beta": "git fetch origin && git checkout beta && git pull origin beta && git pull origin develop && git push origin beta",
    "update:next": "git fetch origin && git checkout next && git pull origin next && git pull origin develop && git push origin next"
  },
  "publishConfig": {
    "registry": "https://artifactory.comcast.com/artifactory/api/npm/Lightning-npm-releases/"
  },
  "description": "A collection of Comcast-branded UI components for the [Lightning](https://rdkcentral.github.io/Lightning/) framework.",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "author": "",
  "license": "ISC"
}
