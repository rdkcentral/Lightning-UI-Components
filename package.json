{
  "name": "@lightning/ui",
  "version": "4.19.3",
  "sideEffects": false,
  "dependencies": {
    "debounce": "^1.2.1"
  },
  "peerDependencies": {
    "@lightningjs/core": "^2.1.1"
  },
  "browser": "index.js",
  "main": "index.js",
  "bin": {
    "bootstrap": "bin/bootstrap.sh"
  },
  "files": [
    "index.js",
    "Styles.js",
    "{bin,elements,layout,mixins,patterns,Styles,textures,utils}/**/*",
    "!{elements,layout,mixins,patterns,Styles}/**/*.stories.js",
    "!{elements,layout,mixins,patterns,textures,utils}/**/*.test.js",
    "test/lightning-test-renderer.js",
    "test/lightning-test-utils.js",
    "!{elements,layout,patterns,textures}/**/__snapshots__",
    "!public/"
  ],
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@lightningjs/core": "*",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-docs": "^6.3.10",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-storysource": "^6.3.10",
    "@storybook/html": "^6.3.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "gh-pages": "^3.1.0",
    "husky": "^3.1.0",
    "jest": "^26.6.1",
    "jest-webgl-canvas-mock": "^0.2.3",
    "lint-staged": "^9.5.0",
    "prettier": "^2.4.1",
    "semantic-release": "^17.4.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./{elements,layout,patterns,utils}/**/*.js": [
      "npm run lint"
    ]
  },
  "repository": {
    "url": "https://github.comcast.com/Lightning/lightning-ui"
  },
  "scripts": {
    "build": "build-storybook -c .storybook -o dist",
    "lint": "eslint './{collections,elements,layout,mixins,patterns,styles,utils}/**/*.js'",
    "lint:fix": "eslint --fix './{collections,elements,layout,mixins,patterns,styles,utils}/**/*.js'",
    "gh-pages": "gh-pages -d dist",
    "create": "sh bin/create.sh",
    "start": "start-storybook -p 8000",
    "start:ci": "start-storybook --ci --smoke-test",
    "test": "jest --coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:watch": "jest --no-coverage --watch",
    "test:updateSnapshot": "jest --updateSnapshot",
    "kick-concourse": "git commit --no-verify --allow-empty -m 'Trigger Concourse' && git push origin",
    "release": "git fetch origin && git checkout develop && git pull origin develop && git pull origin release && git push origin develop && git checkout release && git pull origin release && git pull origin develop && git push origin release",
    "update:next": "git fetch origin && git checkout next && git pull origin next && git pull origin develop && git push origin next",
    "update:beta": "git fetch origin && git checkout beta && git pull origin beta && git pull origin develop && git push origin beta"
  },
  "publishConfig": {
    "registry": "https://artifactory.comcast.com/artifactory/api/npm/Lightning-npm-releases/"
  }
}
